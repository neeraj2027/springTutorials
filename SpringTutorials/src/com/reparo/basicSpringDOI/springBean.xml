<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="emp2" class="com.reparo.basicSpringDOI.Employee">
		<property name="eid" value="12"/>
		<property name="ename" value="AMIT"/>
		<property name="address" ref="add2"/>
	</bean>
	
	<bean id="emp3" class="com.reparo.basicSpringDOI.Employee">
		<property name="eid" value="13"/>
		<property name="ename" value="Vikrant"/>
		<property name="address" ref="add3"/>
	</bean>
	
	<bean id="emp4" class="com.reparo.basicSpringDOI.Employee">
		<property name="eid" value="14"/>
		<property name="ename" value="Rishu"/>
		<property name="address" ref="add4"/>
	</bean>

	<bean id="add2" class="com.reparo.basicSpringDOI.Address">
		<property name="area" value="24"/>
		<property name="city" value="indore"/>
		<property name="country" value="india"/>
	</bean>
	<bean id="add3" class="com.reparo.basicSpringDOI.Address">
		<property name="area" value="23"/>
		<property name="city" value="bhopal"/>
		<property name="country" value="india"/>
	</bean>
	<bean id="add4" class="com.reparo.basicSpringDOI.Address">
		<property name="area" value="54"/>
		<property name="city" value="allahabad"/>
		<property name="country" value="india"/>
	</bean>

</beans>
